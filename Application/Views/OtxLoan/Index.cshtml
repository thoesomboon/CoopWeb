        function TxnCodeChange(tCode) {
            $.post('@Url.Action("TransferInTxnCode", "TxnCode")', { tType: "LON", tCode: tCode }, function (txndata) {
                //debugger
                if (txndata != null) {
                    //debugger
                    //alert("รหัสรายการ ( " + txndata[1].TTXnCode + " " + txndata[1].Descript + ")");
                    TransferInTxnCode(txndata[1]);
                } else {
