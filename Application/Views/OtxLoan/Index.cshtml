                    BFDiscInt: numeric.txt2DBL($("#BFDiscInt").val()),
                    DiscInt: numeric.txt2DBL($("#DiscInt").val()),
                    BFUnpayDiscInt: numeric.txt2DBL($("#BFUnpayDiscInt").val()),
