    <script type="text/javascript">
        $(document).ready(function () {

            toastr.options = {
                "closeButton": true,
                "debug": false,
                "positionClass": "toast-top-right",
                "onclick": null,
                "showDuration": "1000",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }

            var catId = $("li[class='nav-item active open']").attr('parent');
            if (catId != undefined) {
                $("#cat-" + catId).addClass('nav-item active open');
                $("#cat-" + catId + ' a').append('<span class="selected"></span>');
            }
            else {
                var parentCatId = $("span[class='selected']").attr('parent');
                $("#cat-" + parentCatId).addClass('nav-item active open');
            }

            $('#clickmewow').click(function () {
                $('#radio1003').attr('checked', 'checked');
            });

            /// Timeout dialog ////////
            var $countdown;
            $('body').append('');
            // start the idle timer plugin
            $.idleTimeout('#idle-timeout-dialog', '.modal-content button:last', {
                idleAfter: 3600, // 120 seconds
                timeout: 30000, //30 seconds to timeout
                pollingInterval: 5, // 5 seconds
                keepAliveURL: '../../idletimeout_keepalive.html',
                serverResponseEquals: 'OK',
                onTimeout: function () {
                    window.location = "@Url.Action("LogoutExpire", "LogOut")";
                },
                onIdle: function () {
                    $('#idle-timeout-dialog').modal('show');
                    $("#idle-timeout-dialog").removeClass('modal-fix');
                    $countdown = $('#idle-timeout-counter');

                    $('#idle-timeout-dialog-keepalive').on('click', function () {
                        $('#idle-timeout-dialog').modal('hide');
                    });

                    $('#idle-timeout-dialog-logout').on('click', function () {
                        $('#idle-timeout-dialog').modal('hide');
                        $.idleTimeout.options.onTimeout.call(this);
                    });
                },
                onCountdown: function (counter) {
                    $countdown.html(counter); // update the counter
                }
            });
        })

        document.onkeydown = function () {
            if (event.keyCode == 8) {
                if (((event.srcElement.type == 'text' || event.srcElement.type == 'textarea' || event.srcElement.type == 'password' || event.srcElement.type == 'search') && event.srcElement.readOnly == false) || event.srcElement.className == 'note-editable panel-body') {
                    return true;
                } else {
                    return false;
                }
            }
            if (event.keyCode == 13 && event.srcElement.type != 'textarea') {
                return false;
            }
            return true;
        };

    </script>
