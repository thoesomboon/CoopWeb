        $("#Descript").click(function (e) {
            //debugger
            $("#TTxnCode").val($("#Descript").val());
            document.getElementById("TTxnCode").focus();
        });

        $("#TTxnCode").keyup(function (e) {
            //debugger
            if (event.keyCode === 13) {
                e.preventDefault();
                //debugger
                TxnCodeChange(this.value);
                //OtxDepositCalcInt(string AccNo, string BackDate)
