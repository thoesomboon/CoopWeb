            $("#IntType").val("");
            $("#LastCalcIntTH").val("");
            $("#MonthWithdrawAmt").val("");
            $("#MonthWithdrawTimes").val("");
            $("#LastContactTH").val("");
            $("#BookNo").val("");
            $("#LastBookLine").val("");
            $("#HoldAmt").val("");
            $("#TTxnCode").val("");
            $("#Descript").val("");
            $("#TxnDate").val("");
            $("#BackDate").val("");
            $("#IntAmt").val("");
            $("#Amt").val("");
            $("#CFLedgerBal").val("");
            $("#NetAmt").val("");
