        function TxnCodeChange(tCode) {
            $.post('@Url.Action("TransferInTxnCodeDEP", "TxnCode")', { tCode }, function (txndata) {
                //debugger
                if (txndata != null) {
                    //debugger
                    //alert("รหัสรายการ ( " + txndata[1].TTXnCode + " " + txndata[1].Descript + ")");
                    TransferInTxnCode(txndata[1]);
                } else {
                    //alert("ไม่พบข้อมูลรหัสรายการ ( " + tCode + " )");
                    alert("ไม่พบรหัสรายการ (" + tCode + ") โปรดตรวจสอบ!")
                }
            });
        }

        function TransferInTxnCode(data) {
            //debugger
            $("#Descript").val(data.TTxnCode);
            $("#CDCode").val(data.CDCode);
            $("#OCFlag").val(data.OCFlag);
            $("#InstrumentType").val(data.InstrumentType);

            var HoldAmt = numeric.txt2DBL($("#HoldAmt").val());
            var LedgerBal = numeric.txt2DBL($("#HoldAmt").val());
            var AvailBal = numeric.txt2DBL($("#AvailBal").val());

            if (data.CDCode == "D" && data.OCFlag == "C") {
                if ((HoldAmt != null) || (LedgerBal != AvailBal)) {
                    //alert("ไม่สามารถปิดบัญชีได้ เพราะมียอดเงินอายัด หรือมีเช็คเคลียริ่งค้าง --! โปรดตรวจสอบ !--");
                    alert("ไม่สามารถปิดบัญชีได้ เพราะมียอดเงินอายัด หรือมีเช็คเคลียริ่งค้าง โปรดตรวจสอบ!")
                    //$("#btnSave").prop("disabled", false);
                }
            };
        }

        $("#Amt").keyup(function (e) {
            //debugger
            if (event.keyCode === 13) {
                e.preventDefault();
                //debugger
                if (this.value > 0) {
                    $("#ChargeAmt").val(CalcChargeAmt(this.value));
                    CalcTxnAmt(this.value);
                    //Save();
                }
                else {
                    alert("จำนวนเงินไม่ถูกต้อง โปรดตรวจสอบ!")
                }
                //debugger
                //$("#TTxnCode").focus;
            }
        });

        function CalcChargeAmt(tAmt) {
            debugger
            var MonthMaxWithdrawAmt = numeric.txt2DBL($("#MonthMaxWithdrawAmt").val());
            var MonthMaxWithdrawTimes = numeric.txt2DBL($("#MonthMaxWithdrawTimes").val());
            var MinChargeAmt = numeric.txt2DBL($("#MinChargeAmt").val());
            var MaxChargeAmt = numeric.txt2DBL($("#MaxChargeAmt").val());
            var WithdrawChargePercent = numeric.txt2DBL($("#WithdrawChargePercent").val());

            //debugger
            var MonthMaxWithdrawAmt = numeric.txt2DBL($("#MonthMaxWithdrawAmt").val());
            var MonthMaxWithdrawTimes = numeric.txt2DBL($("#MonthMaxWithdrawTimes").val());

            debugger
            var Amt2BeCharged = 0;
            var ChargeAmt = 0;
            if (WithdrawChargePercent != null) {
                if (MonthMaxWithdrawAmt != null && MonthMaxWithdrawAmt + tAmt > MonthMaxWithdrawAmt) {
                    Amt2BeCharged = tAmt;
                }
                if (MonthMaxWithdrawTimes != null && MonthMaxWithdrawTimes + 1 > MonthMaxWithdrawTimes) {
                    Amt2BeCharged = tAmt;
                }
                ChargeAmt = Amt2BeCharged * WithdrawChargePercent / 100;
                if (ChargeAmt < MinChargeAmt) {
                    ChargeAmt = MinChargeAmt;
                }
                else {
                    if (ChargeAmt > MaxChargeAmt) {
                        ChargeAmt = MaxChargeAmt;
                    }
                }
            }
            //debugger
            return ChargeAmt;
        };

        function CalcTxnAmt(TxnAmt) {
            //debugger
            var LedgerBal = numeric.txt2DBL($("#LedgerBal").val());
            var CDCode = $("#CDCode").val();
            var OCFlag = $("#OCFlag").val();
            var Amt = numeric.txt2DBL($("#Amt").val());
            var Debit = 0;
            var Credit = 0;
            var ChargeAmt = numeric.txt2DBL($("#ChargeAmt").val());
            var CfLedgerBal = 0;
            var IntAmt = 0;
            var NetAmt = 0;
            var Filestatus = "A";
            var MinLedgerBal = numeric.txt2DBL($("#MinLedgerBal").val()) ?? 0
                ;
            ChargeAmt = 0;
            debugger
            if (CDCode == "C") {    //ฝาก
                NetAmt = Amt;
                CfLedgerBal = LedgerBal + Amt;
                Credit = Amt;
                }
            else {                                      //ถอน
                if (OCFlag == "C") {                    //ปิดบัญชี
                    //debugger
                    Amt = LedgerBal;
                    IntAmt = numeric.txt2DBL($("#AccInt").val()) ?? 0;
                    NetAmt = LedgerBal + IntAmt - ChargeAmt;
                    CfLedgerBal = 0;
                    Filestatus = "C";
                }
                else {
                    if (Amt > LedgerBal) {
                        Amt = LedgerBal - MinLedgerBal;
                        NetAmt = Amt;
                        CfLedgerBal = MinLedgerBal;
                    }
                    else {
                        NetAmt = Amt - ChargeAmt;
                        CfLedgerBal = LedgerBal - Amt;
                    }
                }
                Debit = NetAmt;
            }
            //debugger
            $("#Amt").val(Amt);
            $("#IntAmt").val(IntAmt);
            $("#ChargeAmt").val(ChargeAmt);
            $("#NetAmt").val(NetAmt);
            $("#CFLedgerBal").val(CfLedgerBal);
            $("#Credit").val(Credit);
            $("#Debit").val(Debit);
            $("#Filestatus").val(Filestatus);
            //debugger
        };

        $("#btnSave").click(function (e) {
            e.preventDefault();
            if (confirm("กรูณายืนยันการบันทึกข้อมูล")) {
                //Save;
                debugger
                var CDCode = $("#CDCode").val();
                var OCFlag = $("#OCFlag").val();

                var TxnAmt = numeric.txt2DBL($("#Amt").val());
                var HoldAmt = numeric.txt2DBL($("#HoldAmt").val());
                var LedgerBal = numeric.txt2DBL($("#LedgerBal").val());
                var AvailBal = numeric.txt2DBL($("#AvailBal").val());

                var MinDepAmt = numeric.txt2DBL($("#MinDepAmt").val());
                var MaxDepAmt = numeric.txt2DBL($("#MaxDepAmt").val());
