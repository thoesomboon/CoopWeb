                if ($("#AccountNo").val() == null) {
                    //error.push("โปรดบันทึกเลชที่บัญชี");
                    msg = msg + "โปรดบันทึกเลชที่บัญชี";
                }
                if ($("#TTxnCode").val().length != 3) {
                    //error.push("รหัสรายการ ( " + $("#TTxnCode").val() + " ) ไม่ถูกต้อง");
                    msg = ("รหัสรายการ ( " + $("#TTxnCode").val() + " ) ไม่ถูกต้อง")
                }
                debugger
                if (CDCode == "C") {
                    if (TxnAmt < MinDepAmt) {
                        //error.push("จำนวนเงินฝาก ( " + addCommas(TxnAmt) + " ) น้อยกว่า ฝากขั้นต่ำที่กำหนด ( " + addCommas(MinDepAmt) + " )");
                        msg = "จำนวนเงินฝาก ( " + addCommas(TxnAmt) + " ) น้อยกว่า ฝากขั้นต่ำที่กำหนด ( " + addCommas(MinDepAmt) + " )";
                    }
                    if (TxnAmt > MaxDepAmt) {
                        //error.push("จำนวนเงินฝาก ( " + addCommas(TxnAmt) + " ) มากกว่า ฝากขั้นสูงที่กำหนด ( " + addCommas(MaxDepAmt) + " )");
                        msg = "จำนวนเงินฝาก ( " + addCommas(TxnAmt) + " ) มากกว่า ฝากขั้นสูงที่กำหนด ( " + addCommas(MaxDepAmt) + " )";
                    }
                }
                if (CDCode == "D") {
                    if (OCFlag == "C") {
                        if ((HoldAmt != null) || (LedgerBal != AvailBal)) {
                            //error.push("ไม่สามารถปิดบัญชีได้ เพราะมียอดเงินอายัด หรือมีเช็คเคลียริ่งค้าง โปรดตรวจสอบ!");
                            msg = "ไม่สามารถปิดบัญชีได้ เพราะมียอดเงินอายัด หรือมีเช็คเคลียริ่งค้าง โปรดตรวจสอบ!";
                        }
                    }
                    debugger
                    if (OCFlag != "C") {
                        if (TxnAmt < MinWithdrawAmt) {
                            //error.push("จำนวนเงินถอน ( " + addCommas(TxnAmt) + " ) น้อยกว่า ถอนขั้นต่ำที่กำหนด ( " + addCommas(MinWithdrawAmt) + " )");
                            msg = "จำนวนเงินถอน ( " + TxnAmt + " ) น้อยกว่า ถอนขั้นต่ำที่กำหนด ( " + MinWithdrawAmt + " )";
                        }
                        if (TxnAmt > MaxWithdrawAmt) {
                            //error.push("จำนวนเงินถอน ( " + addCommas(TxnAmt) + " ) มากกว่า ถอนขั้นสูงที่กำหนด ( " + addCommas(MaxWithdrawAmt) + " )");
                            msg = "จำนวนเงินถอน ( " + TxnAmt + " ) มากกว่า ถอนขั้นสูงที่กำหนด ( " + MaxWithdrawAmt + " )";
                        }
                        if (LedgerBal - TxnAmt < MinLedgerBal) {
                            //error.push("ยอดเงินคงเหลือ หลังถอน ( " + addCommas(LedgerBal - TxnAmt) + " ) น้อยกว่ากำหนด ( " + addCommas(MinLedgerBal) + " ) ต้องปิดบัญชี");
                            msg = "ยอดเงินคงเหลือ หลังถอน ( " + (LedgerBal - TxnAmt) + " ) น้อยกว่ากำหนด ( " + MinLedgerBal + " ) ต้องปิดบัญชี";
                        }
                    }
                }
                if (msg.length > 0) {
                    alert(" โปรดตรวจสอบ! \n" + msg);
                    return;
                }
                //var DepTypeID = $("#DepositTypeName").val();
                var result = false;
                debugger
                var aData = {
                    CoopID: $("#CoopID").val(),
                    AccountNo: $("#AccountNo").val(),
                    MemberID: $("#MemberID").val(),
                    DepositTypeID: $("#DepositTypeName").val(),
                    TxnDateTH: $("#TxnDateTH").val(),
                    BackDateTH: $("#BackDateTH").val(),
                    LedgerBal: numeric.txt2DBL($("#LedgerBal").val()),
                    Amt: numeric.txt2DBL($("#Amt").val()),
                    Credit: numeric.txt2DBL($("#Credit").val()),
                    Debit: numeric.txt2DBL($("#Debit").val()),
                    CFLedgerBal: numeric.txt2DBL($("#CFLedgerBal").val()),
                    TTxnCode: $("#TTxnCode").val(),
                    AccInt: numeric.txt2DBL($("#AccInt").val()),
                    IntAmt: numeric.txt2DBL($("#IntAmt").val()),
                    CDCode: $("#CDCode").val(),
                    Filestatus: $("#Filestatus").val(),
                    InstrumentType: $("#InstrumentType").val(),
                    TypeOfDeposit: $("#TypeOfDeposit").val(),
                    ChargeAmt: numeric.txt2DBL($("#ChargeAmt").val())
                };
                debugger
                $.ajax({
                    url: '/OtxDeposit/TransferOutOtxDeposit/',
                    type: 'POST',
                    contentType: "application/json; charset=utf-8",
                    dataType: 'json',
                    data: JSON.stringify(aData),
                    async: false,
                    cache: false,
                    beforeSend: function (x) {
                        if (x && x.overrideMimeType) {
                            x.overrideMimeType('application/json;charset=UTF-8');
                        }
                    },
                    success: function (data, status, xhr) {
                        if (status == 'success' && data != null && data[0] != null) {
